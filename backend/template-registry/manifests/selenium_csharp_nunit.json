{
    "framework": "selenium",
    "language": "csharp",
    "engine": "nunit",
    "version": "1.0",
    "variables": {
        "SELENIUM_VERSION": {
            "required": false,
            "default": "4.15.0"
        },
        "NUNIT_VERSION": {
            "required": false,
            "default": "3.14.0"
        }
    },
    "folders": [
        {
            "path": "tests",
            "gitkeep": true
        },
        {
            "path": "pages",
            "gitkeep": true
        }
    ],
    "files": [
        {
            "path": "Framework.csproj",
            "type": "fragment",
            "source": "csproj_nunit.txt"
        },
        {
            "path": "tests/ExampleTest.cs",
            "type": "fragment",
            "source": "example_test_cs.txt"
        },
        {
            "path": ".github/workflows/selenium.yml",
            "type": "fragment",
            "source": "ci_selenium_csharp.yml"
        },
        {
            "path": "README.md",
            "type": "fragment",
            "source": "readme_selenium_csharp.md"
        },
        {
            "path": ".gitignore",
            "type": "inline",
            "content": "bin/\nobj/\n.vs/\n.vscode/\n.env"
        }
    ]
}