{
    "framework": "selenium",
    "language": "java",
    "engine": "testng",
    "version": "1.0",
    "variables": {
        "PROJECT_NAME": {
            "required": true,
            "default": "selenium-java-project"
        },
        "SELENIUM_VERSION": {
            "required": false,
            "default": "4.15.0"
        },
        "TESTNG_VERSION": {
            "required": false,
            "default": "7.9.0"
        }
    },
    "folders": [
        {
            "path": "src/main/java",
            "gitkeep": true
        },
        {
            "path": "src/test/java",
            "gitkeep": true
        }
    ],
    "files": [
        {
            "path": "pom.xml",
            "type": "fragment",
            "source": "pom_xml_testng.txt"
        },
        {
            "path": "testng.xml",
            "type": "fragment",
            "source": "testng_xml.txt"
        },
        {
            "path": "src/test/java/tests/ExampleTest.java",
            "type": "fragment",
            "source": "example_test_java.txt"
        },
        {
            "path": ".github/workflows/selenium.yml",
            "type": "fragment",
            "source": "ci_selenium_java.yml"
        },
        {
            "path": "README.md",
            "type": "fragment",
            "source": "readme_selenium_java.md"
        },
        {
            "path": ".gitignore",
            "type": "inline",
            "content": "target/\n*.iml\n.idea/\n.vscode/\n.env"
        }
    ]
}