{
    "framework": "cypress",
    "language": "javascript",
    "engine": "mocha",
    "version": "1.0",
    "variables": {
        "CYPRESS_VERSION": {
            "required": false,
            "default": "13.0.0"
        }
    },
    "folders": [
        {
            "path": "cypress/e2e",
            "gitkeep": true
        },
        {
            "path": "cypress/support",
            "gitkeep": true
        }
    ],
    "files": [
        {
            "path": "package.json",
            "type": "fragment",
            "source": "package_json_cypress.json"
        },
        {
            "path": "cypress.config.js",
            "type": "fragment",
            "source": "cypress_config_js.txt"
        }
    ]
}